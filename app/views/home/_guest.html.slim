  .modal-dialog
    .modal-content
      .modal-body
        form.form-horizontal
          fieldset
            legend Создать гостевой ключ
            .form-group
              label.col-lg-2.control-label for="inputGuestName"  Имя стримера
              .col-lg-10
                input#inputGuestName.form-control type="text" /
            .form-group
              label.col-lg-2.control-label for="inputGuestGame"  Игра
              .col-lg-10
                input#inputGuestGame.form-control type="text" /
            .form-group
              label.col-lg-2.control-label for="inputGuestKinco"  Кинцо
              .col-lg-10
                input#inputGuestKinco.form-control type="text" /
            .form-group
              .col-lg-10
                a.btn.btn-primary href="#" onclick="createGuest();" Создать
